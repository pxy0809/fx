{
  "source": "./src",
  "destination": "./docs/site",
  "plugins": [
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-typescript-plugin",
      "option": {
        "enable": true
      }
    },
    {
      "name": "esdoc-publish-html-plugin",
      "option": {
        "template": "./docs/template"
      }
    }
  ]
}